{
  "name": "ts-standardx",
  "version": "0.6.0-0",
  "description": "Yet another configurable linter for TypeScript and JavaScript.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "bin": "bin/cmd.js",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "clean": "rimraf dist .cache",
    "dts": "tsc -d --emitDeclarationOnly --declarationDir .cache",
    "bundle": "rollup -c",
    "build": "npm run clean && npm run dts && npm run bundle",
    "lint": "npx .",
    "lint:fix": "npx . --fix",
    "test": "npm run build && npm run lint && jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exuanbo/ts-standardx.git"
  },
  "keywords": [
    "standard",
    "standardx",
    "ts-standard",
    "typescript",
    "eslint",
    "lint",
    "prettier"
  ],
  "author": "exuanbo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exuanbo/ts-standardx/issues"
  },
  "homepage": "https://github.com/exuanbo/ts-standardx#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "16.0.2",
    "eslint-config-standard-jsx": "10.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "prettier": "2.2.1",
    "standard-engine-ts": "0.4.0-0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "6.1.0",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.10",
    "jest": "26.6.3",
    "rimraf": "3.0.2",
    "rollup": "2.34.0",
    "rollup-plugin-dts": "2.0.0",
    "ts-jest": "26.4.4",
    "tslib": "2.0.3",
    "typescript": "4.1.2"
  }
}
